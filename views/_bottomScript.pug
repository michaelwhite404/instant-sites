script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/overlayscrollbars/1.13.0/js/jquery.overlayScrollbars.min.js")
script(src="https://unpkg.com/papaparse@latest/papaparse.min.js")
script.
  var csvData;
  $('.hamburger').click(function() {
    $('body').toggleClass("open");
    $(window).trigger('resize');
  });
  $('.full-content-wrapper').on("click", function () {
    if ($('body').hasClass("open")) {
      $('body').toggleClass("open");
      $(window).trigger('resize');
    }
  });
  $(".input, .color-input").on("focus", function () {
    $(this).closest('.input-wrapper').addClass('focused');
  });
  $(".input, .color-input").on("blur", function () {
    $(this).closest('.input-wrapper').removeClass('focused');
  });
  $(".input").on("input", function() {
    if ($(this).val()) {
      $(this).siblings('.field-label').addClass('stay');
    } else {
      $(this).siblings('.field-label').removeClass('stay');
    }
  });
  $(".color-input").on("input", function() {
    if ($(this).val()) {
      $(this).parent().siblings('.field-label').addClass('stay');
    } else {
      $(this).parent().siblings('.field-label').removeClass('stay');
    }
  });
  if ($('body').hasClass('dark')) {
    $("#dark-mode-check").attr('checked',"")
  }
  $("#dark-mode-check").on("change", function() {
    if ($('body').hasClass('dark')) {
      $('body').removeClass('dark');
      $("#dark-mode-check").removeAttr("checked");
      $(".os-host").removeClass("os-theme-light");
      $(".os-host").addClass("os-theme-dark");
    } else {
      $('body').addClass('dark');
      $(".os-host").removeClass("os-theme-dark");
      $(".os-host").addClass("os-theme-light");
    }
  });
  $( document ).ready(function() {
    $( ".input" ).each(function() {
      if ($(this).val()) {
        $(this).siblings('.field-label').addClass('stay');
      }
    });
  });
  $(function() {
  //The passed argument has to be at least a empty object or a object with your desired options
    $(".database-table tbody").overlayScrollbars({ });
    if ($("body").hasClass("dark")) {
      if ($(".os-host")) {
        $(".os-host").removeClass("os-theme-dark");
        $(".os-host").addClass("os-theme-light");
      }
    }
  });
  $(window).resize(function() {
    var extraWidth = 140;
    if ($('body').hasClass('open')) {
      extraWidth = 280;
    }
    var tbodyWidth = $(window).width() - extraWidth + "px";;
    var tbodyHeight = $( window ).height() - 347 + "px";
    $('tbody').css('width',tbodyWidth);
    $('tbody').css('max-height',tbodyHeight);
  });
  $('.color-input').on("input", function() {
    $('#color').val($(this).val());
  });
  $("#color").on("input", function() {
    if ($(this).val().length === 7) {
      $(".color-input").val($(this).val());
    }
  });
  $('#image-upload').on('change', function() {
    if (!$(this).val()) return $('#file-selected').html("No file selected");
    var fileName, fileArray; 
    fileArray = $(this).val().split("\\");
    fileName = fileArray[fileArray.length-1];
    $('#file-selected').html(fileName); 
  });
  var path = window.location.pathname.split('/')[1];
  $('.nav-menu .nav-link').each(function() {
    if ($(this).attr('data-path') == path) {
      $(this).addClass("current");
    }  
  });

script(src="/js/pagination.js" type="text/javascript")
